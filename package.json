{
  "name": "babel-plugin-source",
  "version": "0.0.6",
  "description": "Extract source code to strings at build-time",
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "main": "dist/index.js",
  "scripts": {
    "add-contributor": "kcd-scripts contributors add",
    "build": "kcd-scripts build",
    "lint": "eslint src",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate",
    "precommit": "yarn validate"
  },
  "files": [
    "dist",
    "macro.js"
  ],
  "keywords": [
    "babel",
    "babel-plugin",
    "source",
    "code",
    "babel-plugin-macros"
  ],
  "author": "Tanner Linsley <tannerlinsley@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "babel-plugin-macros": "^2.0.0",
    "babel-register": "^6.26.0",
    "babylon": "^6.18.0",
    "eslint-config-prettier": "^4.3.0",
    "require-from-string": "^2.0.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "ast-pretty-print": "^2.0.1",
    "babel-eslint": "9.x",
    "babel-plugin-tester": "^6.2.1",
    "eslint": "5.x",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.5.0",
    "kcd-scripts": "^1.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "babel": {
    "presets": [
      "kcd-scripts/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tannerlinsley/babel-plugin-source.git"
  },
  "bugs": {
    "url": "https://github.com/tannerlinsley/babel-plugin-source/issues"
  },
  "homepage": "https://github.com/tannerlinsley/babel-plugin-source#readme"
}
